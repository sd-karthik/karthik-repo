/* Output after compiling both the files, sony2a.c and sony2b.c */
/* sony2a.c */
#include	<stdio.h>
extern int arr[25];
extern int fun(int *, int );
int main()
{
	fun(arr, sizeof(arr));
}
        
         /* sony2b.c */
int arr[15];
int fun(int arr[], int len)
{
	if(len > 10)
		printf("HELLO\n");
}

