#ifndef HEADER_H
#define HEADER_H

#include<stdio.h>
#include<stdlib.h>
#include<string.h>

#define MAX 10

typedef struct NODE                                                             
{                                                                               
    char data;                                                                  
    struct NODE *next;                                                          
}ST;     

ST *head; 
void inser_begin(ST **);
void display(ST **);
void inser_at_end(ST **);
int inser_at_pos(ST **);
int inser_befre_at_pos(ST **);
int inser_after_at_number(ST **);
int inser_before_at_number(ST **);
int inser_after_at_pos(ST **);
int inser_at_middle(ST **);
int inser_at_penultimate_node(ST **);
void del_begin(ST **);
void del_at_end(ST **);
int del_at_pos(ST **);
int del_befre_at_pos(ST **);
int del_after_at_number(ST **);
int del_before_at_number(ST **);
int del_after_at_pos(ST **);
int del_at_middle(ST **);
int del_at_penultimate_node(ST **);

int nodes;
